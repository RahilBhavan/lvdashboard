# ============================================
# DEVELOPMENT ENVIRONMENT VARIABLES
# ============================================
# Copy this file to .env or .env.local for local development
# Safe to use test keys and test networks here

# ============================================
# BLOCKCHAIN CONFIGURATION (Testnet)
# ============================================
# Sepolia testnet RPC URL
RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Network Configuration
CHAIN_ID=11155111
NETWORK_NAME=sepolia

# ============================================
# DEPLOYED CONTRACT ADDRESSES (Testnet)
# ============================================
# Sepolia testnet addresses
VAULT_ADDRESS=0xYOUR_SEPOLIA_VAULT_ADDRESS
TOKEN0=0x1c7D4B196Cb0C7B01d743Fbc6116a902379C7238
TOKEN1=0xfFf9976782d46CC05630D1f6eBAb18b2324d6B14
POSITION_MANAGER=0x1238536071E1c677A632429e3655c799b22cDA52
KEEPER_ADDRESS=0xYOUR_SEPOLIA_KEEPER_ADDRESS

# ============================================
# KEEPER BOT CONFIGURATION (Local/Dev)
# ============================================
# Test wallet private key (use a dedicated test wallet!)
# Fund with Sepolia ETH from: https://sepoliafaucet.com/
KEEPER_PK=0xYOUR_TEST_KEEPER_PRIVATE_KEY

# Deployer private key (for contract deployment)
PRIVATE_KEY=0xYOUR_TEST_DEPLOYER_PRIVATE_KEY

# Keeper bot parameters (more aggressive for testing)
MIN_PROFIT_THRESHOLD=0.01
CYCLE_TIME=300
MAX_GAS_PRICE=50

# ============================================
# PROTOCOL GOVERNANCE
# ============================================
PROTOCOL_OWNER=0xYOUR_TEST_PROTOCOL_OWNER_ADDRESS

# ============================================
# SUPABASE CONFIGURATION (Development)
# ============================================
# Use a separate Supabase project for development
# Or use the same project with different tables
SUPABASE_URL=https://your-dev-project.supabase.co
SUPABASE_KEY=your-dev-anon-public-key
SUPABASE_SERVICE_KEY=your-dev-service-role-key

# ============================================
# FRONTEND ENVIRONMENT VARIABLES (Local Dev)
# ============================================
# These are used by Vite during local development
VITE_VAULT_ADDRESS=0xYOUR_SEPOLIA_VAULT_ADDRESS
VITE_SUPABASE_URL=https://your-dev-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-dev-anon-public-key
VITE_PROTOCOL_OWNER=0xYOUR_TEST_PROTOCOL_OWNER_ADDRESS
VITE_CHAIN_ID=11155111
VITE_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# WalletConnect Project ID
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# ============================================
# OPTIONAL: EXTERNAL APIS
# ============================================
# Free tier is fine for development
COINGECKO_API_KEY=

# Sepolia Etherscan API
ETHERSCAN_API_KEY=your-etherscan-api-key

# ============================================
# DEVELOPMENT TOOLS
# ============================================
# Enable debug logging
DEBUG=true
LOG_LEVEL=debug

# Mock data for testing (set to true to use mock data instead of real blockchain)
USE_MOCK_DATA=false
